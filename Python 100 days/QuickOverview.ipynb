     "text": [
      "True\n",
      "['Red', 'Green', 'Blue', 'Alph']\n"
     ]
    }
   ],
   "source": [
    "rgb = [\"Red\", \"Green\", \"Blue\"]\n",
    "rgba = rgb\n",
    "q = id(rgb) == id(rgba)  # 它们指向同一个对象\n",
    "print(q)  # True\n",
    "rgba.append(\"Alph\")\n",
    "print(rgb)"
   ]
  }
